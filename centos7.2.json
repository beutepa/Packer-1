{
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "<esc>",
        "<wait>",
        "linux inst.ks=http://{{.HTTPIP}}:{{.HTTPPort}}/ks.cfg",
        "<enter>"
      ],
      "boot_wait": "5s",
      "disk_size": 81920,
      "guest_os_type": "RedHat_64",
      "headless": true,
      "output_directory": "output-virtualbox-centos-7.2",
      "vm_name": "packer-centos-7.2",
      "http_directory": "http",
      "iso_urls": [
        "iso/CentOS-7-x86_64-Minimal-1511.iso",
        "http://www.mirrorservice.org/sites/mirror.centos.org/7.2.1511/isos/x86_64/CentOS-7-x86_64-Minimal-1511.iso"
      ],
      "iso_checksum_type": "sha256",
      "iso_checksum": "f90e4d28fa377669b2db16cbcb451fcb9a89d2460e3645993e30e137ac37d284",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "sudo systemctl poweroff",
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "virtualbox_version_file": ".vbox_version",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "512"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ]
      ]
    },
    {
      "type": "vmware-iso",
      "boot_command": [
        "<esc>",
        "<wait>",
        "linux inst.ks=http://{{.HTTPIP}}:{{.HTTPPort}}/ks.cfg",
        "<enter>"
      ],
      "boot_wait": "5s",
      "disk_size": 81920,
      "output_directory": "output-vmware-centos-7.2",
      "vm_name": "packer-centos-7.2",
      "guest_os_type": "centos-64",
      "headless": false,
      "http_directory": "http",
      "iso_urls": [
        "iso/CentOS-7-x86_64-Minimal-1511.iso",
        "http://www.mirrorservice.org/sites/mirror.centos.org/7.2.1511/isos/x86_64/CentOS-7-x86_64-Minimal-1511.iso"
      ],
      "iso_checksum_type": "sha256",
      "iso_checksum": "f90e4d28fa377669b2db16cbcb451fcb9a89d2460e3645993e30e137ac37d284",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "sudo systemctl poweroff",
      "tools_upload_flavor": "linux",
      "vmx_data": {
        "cpuid.coresPerSocket": "1",
        "memsize": "512",
        "numvcpus": "2"
      }
    }
  ]
}
